<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background color of the button -->
    <solid android:color="#FFFFFF" />

    <!-- Border color and width -->
    <stroke
        android:width="1dp"
    android:color="#0A2E41" />

    <!-- Corner radius to make the button rounded -->
    <corners android:radius="30dp" />

    <!-- Padding for the content inside the button (optional) -->
    <padding
        android:left="4dp"
        android:top="0dp"
        android:right="4dp"
        android:bottom="0dp" />
</shape>
